<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CONTROLADORES.VentanaPrincipal">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Button mnemonicParsing="false" onAction="#agregarProducto" prefWidth="200.0" text="Agregar Producto" />
        <Button mnemonicParsing="false" onAction="#eliminarProducto" prefWidth="200.0" text="Eliminar Producto" />
        <Button mnemonicParsing="false" onAction="#modificarProducto" prefWidth="200.0" text="Modificar Producto" />
        <Button mnemonicParsing="false" onAction="#venta" prefWidth="200.0" text="Venta" />
        <Button mnemonicParsing="false" onAction="#inventario" prefWidth="200.0" text="Inventario" />
    </children>
</VBox>